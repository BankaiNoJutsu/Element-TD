<root>
    <styles>
        <include src="file://{resources}/styles/custom_game/profile.css" />
    </styles>
    <Panel hittest="false" class="ProfileMatch">
        <Label id="MatchID" class="MatchID_Col" text="" />
        <Label id="MatchScore" class="MatchScore_Col" text="" />
        <Panel id="MatchModes" class="MatchModes_Col">
            <!--
                AP/SR/AR
                N/H/VH/I
                R
                C
            -->
        </Panel>

        <Panel id="ElementsContainer" class="MatchElements_Col">
            <Panel id="ElemSubContainer">
                <Label id="HasRandomed" class='hide' text=""/>
                <Panel id="light" class="ElemImageAndLevel">
                    <Image class="ElementImage" src="file://{images}/spellicons/elements/light.png"/>
                    <Label id="light_level" text=""/>
                </Panel>
                <Panel id="dark" class="ElemImageAndLevel">
                    <Image class="ElementImage" src="file://{images}/spellicons/elements/dark.png"/>
                    <Label id="dark_level" text=""/>
                </Panel>
                <Panel id="water" class="ElemImageAndLevel">
                    <Image class="ElementImage" src="file://{images}/spellicons/elements/water.png"/>
                    <Label id="water_level" text=""/>
                </Panel>
                <Panel id="fire" class="ElemImageAndLevel">
                    <Image class="ElementImage" src="file://{images}/spellicons/elements/fire.png"/>
                    <Label id="fire_level" text=""/>
                </Panel>
                <Panel id="nature" class="ElemImageAndLevel">
                    <Image class="ElementImage" src="file://{images}/spellicons/elements/nature.png"/>  
                    <Label id="nature_level" text=""/>
                </Panel>
                <Panel id="earth" class="ElemImageAndLevel">
                    <Image class="ElementImage" src="file://{images}/spellicons/elements/earth.png"/>
                    <Label id="earth_level" text=""/>
                </Panel>
            </Panel>
        </Panel>                     
    </Panel>
</root>